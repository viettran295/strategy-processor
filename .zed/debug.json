// Project-local debug tasks
//
// For more documentation on how to configure debug tasks,
// see: https://zed.dev/docs/debugger
[
  {
    "label": "Build & Debug Rust",
    "build": {
      "command": "cargo",
      "args": ["build"]
    },
    "program": "$ZED_WORKTREE_ROOT/target/debug/str-proc",
    "sourceLanguages": ["rust"],
    "request": "launch",
    "adapter": "CodeLLDB",
    "preRunCommands": [
      "command script import $ZED_WORKTREE_ROOT/.zed/rust_prettifier_for_lldb.py"
    ]
  }
]
